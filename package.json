{
  "name": "sentry-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://taeyzzz.github.io/react-sentry-env",
  "dependencies": {
    "@sentry/browser": "^5.6.3",
    "gh-pages": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "start": "env-cmd -f ./env/.env.development react-scripts start",
    "build": "env-cmd -f ./env/.env.production react-scripts build",
    "build:staging": "env-cmd -f ./env/.env.staging react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.0.1"
  }
}
